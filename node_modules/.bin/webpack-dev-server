#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules/webpack-dev-server/bin/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules/webpack-dev-server/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/node_modules:/Users/zhanghanzhong/Downloads/node_modules:/Users/zhanghanzhong/node_modules:/Users/node_modules:/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules/webpack-dev-server/bin/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules/webpack-dev-server/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/node_modules:/Users/zhanghanzhong/Downloads/node_modules:/Users/zhanghanzhong/node_modules:/Users/node_modules:/node_modules:/Users/zhanghanzhong/Downloads/vuepress_good_nice/blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
